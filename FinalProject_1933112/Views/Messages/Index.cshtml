@model IEnumerable<FinalProject_1933112.Models.Message>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Message1)
        </th>
        <th>
            <text>Manager's </text>@Html.DisplayNameFor(model => model.Manager.FirstName)
        </th>
        <th>
            <text>Tenants's </text>@Html.DisplayNameFor(model => model.Tenant.firstName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Message1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Manager.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tenant.firstName)
        </td>
        
    </tr>
}

</table>
